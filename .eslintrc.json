{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"contentTransfer": "readonly",
		"jQuery": "readonly",
		"mediaWiki": "readonly"
	},
	"rules": {
		"max-len": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-parse-html-literal": "off",
		"no-prototype-builtins": "off"
	}
}
